{
  "name": "react-electron",
  "version": "0.1.0",
  "private": true,
  "author": "John Doe",
  "description": "My fantastic Electron app",
  "main": "public/electron.js",
  "build": {
    "appId": "com.electron.myapp",
    "productName": "YGO",
    "files": [
      "build/**/*"
    ],
    "extraMetadata": {
      "main": "build/static/js/electron.main.js"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  },
  "scripts": {
    "start": "webpack-dev-server --disable-host-check --mode development --inline --progress",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server",
    "electron:start": "concurrently -k \"yarn start\" \"wait-on http://localhost:8002 && electronmon .\"",
    "electron:package:mac": "yarn build && electron-builder -m",
    "electron:package:win": "yarn build && electron-builder -w",
    "electron:package:linux": "yarn build && electron-builder -l",
    "electron:build": "yarn build && electron-builder build --publish=always"
  },
  "keywords": [
    "reactjs",
    "filter",
    "sort",
    "DataTables",
    "jQuery",
    "table",
    "DataTables"
  ],
